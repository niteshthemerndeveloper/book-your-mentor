{
  "name": "cs-group-project",
  "version": "6.4.2022",
  "description": "\"This is a CS 5130 Group Project that allows user to book a mentor, access their mentor orders, and pay through stripe\"",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "CS",
    "Group",
    "MongoDB",
    "Json",
    "Web",
    "Token",
    "Stripe",
    "API",
    "Authentication",
    "and",
    "Validation",
    "Nodejs",
    "HTML",
    "CSS",
    "JavaScript"
  ],
  "author": "Yamini Vemireddy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10",
    "nodemailer": "^6.7.3",
    "stripe": "^8.216.0"
  }
}
